  "author": "",
  "license": "MIT",
  "dependencies": {
    "decimal.js": "^10.4.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.5",
    "cpy-cli": "^6.0.0",
    "jest": "^30.2.0",
    "peggy": "^5.0.6",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.3"
  }
}
