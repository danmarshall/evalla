{
  "name": "evalla",
  "version": "0.1.0",
  "description": "Safe math evaluator with variables, dependencies, and precision",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "peggy --output src/parser.js --format commonjs src/grammar.pegjs",
